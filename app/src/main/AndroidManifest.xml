<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reflex.testeventbus">

    <application
        android:name="com.example.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".TestService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.example.lmw.annotation.testeventbus.TestService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".MainActivity"></activity>
        <activity android:name="com.example.lmw.annotation.testeventbus.MainActivity"></activity>
        <activity android:name=".Main2Activity"></activity>
        <activity android:name="com.example.lmw.annotation.testeventbus.Main2Activity"></activity>
    </application>

</manifest>
